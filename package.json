{
  "name": "hardocs-graphql-api",
  "version": "0.0.4",
  "description": "An API that allows to do mutations meant to be used in an electron app.",
  "scripts": {
    "start": "ts-node --files src",
    "dev": "env NODE_ENV=development nodemon --exec ts-node --files src/index.ts",
    "lint": "eslint './**/*.{tsx,ts}' --quiet --fix",
    "test": "env NODE_ENV=test jest",
    "gen:types": "ts-node --files ./src/scripts/createTypes.ts",
    "build": "rimraf -fr ./dist && tsc && copyfiles -u 1 src/**/*.graphql dist"
  },
  "keywords": [
    "Hardocs"
  ],
  "main": "dist/index.js",
  "author": "Divine Hycenth <divinehycenth8@gmail.com>",
  "license": "GPLv2",
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/express-rate-limit": "^5.0.0",
    "@types/fs-extra": "^9.0.1",
    "@types/glob": "^7.1.3",
    "@types/jsdom": "^16.2.3",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.0.20",
    "@types/showdown": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "copyfiles": "^2.3.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@akryum/winattr": "^3.0.0",
    "@gql2ts/from-schema": "^2.0.0-4",
    "@gql2ts/types": "^1.9.0",
    "apollo-server-express": "^2.16.0",
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "got": "^11.5.0",
    "graphql-import": "^1.0.2",
    "graphql-tools": "6.0.14",
    "graphql-yoga": "^1.18.3",
    "gray-matter": "^4.0.2",
    "jsdom": "^16.4.0",
    "merge-graphql-schemas": "^1.7.8",
    "mime-types": "^2.1.27",
    "nano": "^8.2.2",
    "request": "^2.88.2",
    "showdown": "^1.9.1",
    "winattr": "^3.0.0",
    "yaml": "^1.10.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
