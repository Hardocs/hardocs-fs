    "ts-node": "^8.10.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "jsdom": "^16.4.0",
    "mime-types": "^2.1.27",
